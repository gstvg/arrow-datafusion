#############
## Array Expressions Tests
#############

statement ok
set datafusion.sql_parser.dialect = databricks;

query ?
SELECT list_map([1,2,3,4,5], v -> v*2);
----
[2, 4, 6, 8, 10]

query ?
SELECT list_map([1,2,3,4,5], v -> 2);
----
[2, 2, 2, 2, 2]

query ?
SELECT list_map([[1,2],[3,4,5]], v -> list_map(v, v -> v*2));
----
[[2, 4], [6, 8, 10]]

query ?
SELECT list_map([1,2,3,4,5], v -> repeat("a", v));
----
[a, aa, aaa, aaaa, aaaaa]

query ?
SELECT list_map([1,2,3,4,5], v -> list_repeat("a", v));
----
[[a], [a, a], [a, a, a], [a, a, a, a], [a, a, a, a, a]]
